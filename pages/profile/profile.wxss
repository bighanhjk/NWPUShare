/* pages/profile/profile.wxss */
.login-wrapper{
    height: 400rpx;
    display: flex;
    position: relative;
    justify-content: center;
    align-items: center;
}
.login-wrapper .topimage {
    position: absolute;
    left: 0;
    top: 0;
    z-index: -1;
    width: 100%;
    height: 100%;
}
.login-wrapper .login {
    color: #6C63FF;
    background: #FFFFFF;
    border-radius: 60rpx;
    font-style: 50rpx;
    height: 100rpx;
    line-height: 100rpx;
    width: 350rpx;
    text-align: center;
}
.bottom_left_image{
    
}
.version {
    position: absolute;
    bottom: 30rpx;
    color: #C9C9C9;
    font-style: 24rpx;
    width: 100%;
    text-align: center;
}